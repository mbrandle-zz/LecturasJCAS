<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="lecturasjcas.jcas.gob.lecturasjcas.Lecturas"
    tools:showIn="@layout/app_bar_lecturas"
    android:background="@color/fondo"
    android:orientation="vertical">

    <LinearLayout android:layout_height="wrap_content"
        android:id="@+id/tableRow1"
        android:layout_width="match_parent">

        <TextView
            android:id="@+id/textView1"
            android:textColor="#FFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cuenta                                         "/>

        <TextView android:id="@+id/textView2"
            android:textColor="#FFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ruta"/>
    </LinearLayout>
    <LinearLayout android:layout_height="wrap_content"
        android:id="@+id/tableRow2"
        android:layout_width="fill_parent">

        <EditText android:id="@+id/TCuenta"
            android:background="#FFF"
            android:singleLine="true"
            android:layout_width="125sp"
            android:layout_height="wrap_content"
            android:editable="false"
            android:enabled="true"/>

        <TextView android:text="                 "
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <EditText android:background="#FFF"
            android:layout_width="85sp"
            android:singleLine="true"
            android:layout_height="wrap_content"
            android:id="@+id/TRuta"
            android:editable="false"
            android:enabled="true">

            <requestFocus></requestFocus>
        </EditText>
    </LinearLayout>
    <LinearLayout android:layout_height="wrap_content"
        android:id="@+id/tableRow3"
        android:layout_width="fill_parent">

        <TextView android:text="Nombre"
            android:textColor="#FFF"
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout android:layout_height="wrap_content"
        android:id="@+id/tableRow4"
        android:layout_width="fill_parent">

        <EditText android:background="#FFF"
            android:layout_width="315sp"
            android:singleLine="true"
            android:layout_height="wrap_content"
            android:id="@+id/TNombre"
            android:enabled="true"
            android:editable="false"/>

    </LinearLayout>
    <LinearLayout android:id="@+id/tableRow5"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <TextView android:id="@+id/textView5"
            android:textColor="#FFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Direccion"/>

    </LinearLayout>
    <LinearLayout
        android:id="@+id/tableRow6"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <EditText android:id="@+id/TDireccion"
            android:background="#FFF"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:layout_width="315sp"
            android:enabled="true"
            android:editable="false"/>

    </LinearLayout>
    <LinearLayout
        android:id="@+id/tableRow7"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textView6"
            android:textColor="#FFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Medidor                               "/>

        <TextView android:id="@+id/lblLAnt"
            android:textColor="#FFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="L. Anterior"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/tableRow8"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <EditText
            android:layout_width="120sp"
            android:background="#FFF"
            android:singleLine="true"
            android:layout_height="wrap_content"
            android:id="@+id/TMedidor"
            android:enabled="true"
            android:editable="false"/>

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="          "/>

        <EditText
            android:layout_width="90sp"
            android:background="#FFF"
            android:singleLine="true"
            android:layout_height="wrap_content"
            android:id="@+id/TLectant"
            android:enabled="true"
            android:editable="false"/>

    </LinearLayout>
    <LinearLayout
        android:id="@+id/tableRow9"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textView9"
            android:textColor="#FFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Promedio                             "/>

        <TextView
            android:id="@+id/textView10"
            android:textColor="#FFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="L. Actual"/>


    </LinearLayout>
    <LinearLayout
        android:id="@+id/tableRow10"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <EditText
            android:id="@+id/TPromedio"
            android:background="#FFF"
            android:layout_width="120sp"
            android:singleLine="true"
            android:layout_height="wrap_content"
            android:enabled="true"
            android:editable="false"/>

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="           "/>
        <EditText
            android:id="@+id/TLectact"
            android:background="#FFF"
            android:focusable="true"
            android:singleLine="true"
            android:layout_width="90sp"
            android:layout_height="wrap_content"
            android:numeric="integer"/>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/tableRow11"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textView12"
            android:textColor="#FFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Problema"/>



    </LinearLayout>
    <LinearLayout
        android:id="@+id/tableRow12"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <EditText
            android:numeric="integer"
            android:id="@+id/TProblema"
            android:background="#FFF"
            android:layout_width="50sp"
            android:singleLine="true"
            android:layout_height="wrap_content"/>



        <android.support.design.widget.FloatingActionButton
            android:id="@+id/btnAnt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_margin"
            android:src="@android:drawable/ic_media_previous" />

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/btnGuardar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_margin"
            android:src="@android:drawable/ic_menu_save" />
        <android.support.design.widget.FloatingActionButton
            android:id="@+id/btnSig"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_margin"
            android:src="@android:drawable/ic_media_next"
            />


    </LinearLayout>
    <LinearLayout
        android:id="@+id/tableRow13"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center">

        android.support.design.widget.FloatingActionButton
            android:id="@+id/btnAnt2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_margin"
            android:src="@android:drawable/ic_media_previous" />



        <android.support.design.widget.FloatingActionButton
            android:id="@+id/btnCam"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_margin="@dimen/fab_margin"
            android:src="@android:drawable/ic_menu_camera"/>

        android.support.design.widget.FloatingActionButton
            android:id="@+id/btnSig2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_margin"
            android:src="@android:drawable/ic_media_next"
            />

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/btnFin"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_margin="@dimen/fab_margin"
            android:src="@android:drawable/ic_media_ff"/>
    </LinearLayout>

    <LinearLayout
        android:layout_height="wrap_content"
        android:id="@+id/tableRow16"
        android:layout_width="fill_parent">

        <TextView android:id="@+id/LblPosLatitud"
            android:textColor="#FFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView android:id="@+id/LblPosLongitud"
            android:textColor="#FFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView android:id="@+id/LblPosPrecision"
            android:textColor="#FFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </LinearLayout>



</LinearLayout>
